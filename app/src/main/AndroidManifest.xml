<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />

    <queries>
        <intent>
            <action android:name="android.intent.action.TTS_SERVICE" />
        </intent>
    </queries>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/icon_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_config"
        android:roundIcon="@mipmap/icon_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.ZhongYouGuanJia"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity
            android:name=".ui.splash.SplashActivity"
            android:exported="true"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <!--                <category android:name="android.intent.category.DEFAULT" />-->
                <!--                <category android:name="android.intent.category.HOME" />-->
            </intent-filter>
        </activity>
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".ui.main.MainActivity" />
        <activity
            android:name=".ui.cashier.refueling.RefuelingCashierActivity"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name=".ui.coupon.CouponActivity" />
        <activity android:name=".ui.member.register.MemberRegisterActivity" />
        <activity android:name=".ui.cashier.fast.FastCashierActivity" />
        <activity android:name=".ui.shop.shop.ShopActivity" />
        <activity android:name=".ui.shop.manage.ShopManageActivity" />
        <activity android:name=".ui.wallet.WalletRechargeActivity" />
        <activity android:name=".ui.member.manage.MemberManageActivity" />
        <activity android:name=".ui.car.manage.CarBrandManageActivity" />
        <activity android:name=".ui.member.information.MemberInformationActivity" />
        <activity android:name=".ui.car.add.AddCarBrandActivity" />
        <activity android:name=".ui.member.consume.MemberConsumeActivity" />
        <activity android:name=".ui.member.reset.MemberRechargeRecordActivity" />
        <activity android:name=".ui.member.exchange.MemberExchangeRecordActivity" />
        <activity android:name=".ui.member.integral.MemberIntegralRecordActivity" />
        <activity android:name=".ui.handover.HandoverActivity" />
        <activity android:name=".ui.exchange.gift.GiftExchangeActivity" />
        <activity android:name=".ui.writeoff.WriteOffActivity" />
        <activity android:name=".ui.writeoff.record.WriteOffRecordActivity" />
        <activity android:name=".ui.integral.ManualIntegralActivity" />
        <activity android:name=".ui.personal.PersonalActivity" />
        <activity android:name=".ui.confirm.ConfirmCardActivity" />
        <activity android:name=".ui.change.ChangePassActivity" />
        <activity android:name=".ui.oil.OilSettingActivity" />
        <activity android:name=".ui.print.PrintSettingActivity" />
        <activity android:name=".ui.kpi.KpiSettingActivity" />
        <activity android:name=".ui.member.level.MemberLevelActivity" />
        <activity android:name=".ui.achievement.AchievementActivity" />
        <activity android:name=".ui.scan.ScanCodeActivity" />
        <activity android:name=".ui.search.SearchActivity" />
        <activity android:name=".ui.order.collection.CollectionOrderActivity" />
        <activity android:name=".ui.complete.collection.CompleteOrderActivity" />
        <activity android:name=".ui.order.wallet.WalletOrderActivity" />
        <activity android:name=".ui.complete.wallet.WalletCompleteOrderActivity" />
        <activity android:name=".ui.record.RecordDetailsActivity" />
        <activity android:name=".ui.reconciliation.ReconciliationActivity" />
        <activity android:name=".ui.reconciliation.details.ReconciliationDetailsActivity" />
        <activity android:name=".ui.achievements.AchievementsActivity" />
        <activity android:name=".ui.above.AboveActivity" />
        <activity android:name=".ui.voice.VoicePageActivity" />
        <activity android:name=".ui.cashier.goods.GoodsCashierActivity"/>

        <receiver
            android:name=".receiver.NetworkConnectChangedReceiver"
            android:exported="true">
            <intent-filter>
                <action android:name="android.NET.conn.CONNECTIVITY_CHANGE" />
                <action android:name="android.Net.wifi.WIFI_STATE_CHANGED" />
                <action android:name="android.net.wifi.STATE_CHANGE" />
            </intent-filter>
        </receiver>
    </application>
    <queries>
        <package android:name="woyou.aidlservice.jiuiv5" />
    </queries>
</manifest>